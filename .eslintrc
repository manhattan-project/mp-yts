{
  'extends': 'standard',
  'parser': 'babel-eslint',
  'plugins': [
    'standard'
  ],
  'rules': {
    'curly': 0,
    'no-return-assign': 'off',
    'comma-dangle': 'off',
    'jsx-quotes': [2, 'prefer-double'],
    'no-unused-vars': 'off',
    'padded-blocks': 'off',
    'no-undef': 'off'
  }
}
